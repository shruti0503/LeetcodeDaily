//BRUTE FORCE

class Solution {
public:
    int bulbSwitch(int n) {
        int count = 0;
        for (int i = 1; i <= n; i++) {
            int factors = 0;
            for (int j = 1; j <= sqrt(i); j++) {
                if (i % j == 0) {
                    factors++;
                    if (j != i / j) {
                        factors++;
                    }
                }
            }
            if (factors % 2 == 1) {
                count++;
            }
        }
        return count;
    }
};
